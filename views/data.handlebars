<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>

<body>

    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
                <th scope="col">Experience</th>
                <th scope="col">Reviews</th>
                <th scope="col">Rating</th>
                <th scope="col">Bio</th>
                <th scope="col">Subject</th>
                <th scope="col">Location</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
            <tr>
                <td>{{id}}</td>
                <td>{{tutor_names}}</td>
                <td>{{age}}</td>
                <td>{{experience}}</td>
                <td>{{reviews}}</td>
                <td>{{tutor_rating}}</td>
                <td>{{tutors_bio}}</td>
                <td>{{tutor_subject}}</td>
                <td> {{tutros_location}}</td>
            </tr>
            {{/each}}

        </tbody>
    </table>


</body>

</html>